<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <DefaultOutputDir>$(SolutionDir)bin\$(Configuration)-$(Platform)\$(ProjectName)\</DefaultOutputDir>
    <DefaultIntDir>$(SolutionDir)bin-int\$(Configuration)-$(Platform)\$(ProjectName)\</DefaultIntDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="DefaultOutputDir">
      <Value>$(DefaultOutputDir)</Value>
    </BuildMacro>
    <BuildMacro Include="DefaultIntDir">
      <Value>$(DefaultIntDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>